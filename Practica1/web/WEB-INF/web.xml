<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>p1.email.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarAlumnoServlet</servlet-name>
        <servlet-class>p1.email.RegistrarAlumnoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarMatriculaDB</servlet-name>
        <servlet-class>RegistrarMatriculaDB</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarMatriculaServlet</servlet-name>
        <servlet-class>p1.email.RegistrarMatriculaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarAlumnoServlet</servlet-name>
        <url-pattern>/registroAlumno</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarMatriculaDB</servlet-name>
        <url-pattern>/RegistrarMatriculaDB</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarMatriculaServlet</servlet-name>
        <url-pattern>/RegistrarMatriculaServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
